game:GetService("Players").PlayerAdded:Wait()

wait(3)

local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/GreenDeno/Venyx-UI-Library/main/source.lua"))()
local venyx = library.new("WrldHub", 5013109572)

-- themes
local themes = {
Background = Color3.fromRGB(24, 24, 24),
Glow = Color3.fromRGB(0, 0, 0),
Accent = Color3.fromRGB(10, 10, 10),
LightContrast = Color3.fromRGB(20, 20, 20),
DarkContrast = Color3.fromRGB(14, 14, 14),  
TextColor = Color3.fromRGB(255, 255, 255)
}

-- first page
local page = venyx:addPage("Main", 5012544693)
local section1 = page:addSection("AutoTime")
local section2 = page:addSection("Old autotime")
local section10 = page:addSection("Other")

section1:addSlider("Ping Timing", 50, 0, 250, function(s)
    getgenv().Time = s
end)

section1:addButton("AutoShoot", function()
	getgenv().ActivationKeybind = Enum.KeyCode.E
	
	
	loadstring(game:HttpGet("https://raw.githubusercontent.com/BootBandit/Rb-world-4/main/test", true))()
end)

local selectedtime1;
local selectedtime2;
local selectedtime3;

section2:addTextbox("Shooting AutoTime adjust", " about 0.86", function(txt, focusLost)
    selectedtime1 = (txt)
end)

section2:addKeybind("Autoshoot", Enum.KeyCode.Clear, function()
    time = (selectedtime1)
    
    local args = {
        [1] = "Shoot",
        [2] = true
    }
    
    game:GetService("ReplicatedStorage").GameEvents.ClientAction:FireServer(unpack(args))
    wait(time)
    
    local args = {
        [1] = "Shoot",
        [2] = false
    }
    
    game:GetService("ReplicatedStorage").GameEvents.ClientAction:FireServer(unpack(args))
end)

section2:addTextbox("Lays AutoTime adjust", " about 0.75", function(txt, focusLost)
    selectedtime2 = (txt)
end)

section2:addKeybind("Autoshoot", Enum.KeyCode.Clear, function()
    time = (selectedtime2)
    
    local args = {
        [1] = "Shoot",
        [2] = true
    }
    
    game:GetService("ReplicatedStorage").GameEvents.ClientAction:FireServer(unpack(args))
    wait(time)
    
    local args = {
        [1] = "Shoot",
        [2] = false
    }
    
    game:GetService("ReplicatedStorage").GameEvents.ClientAction:FireServer(unpack(args))
end)

section2:addTextbox("Other AutoTime adjust", "anything u want", function(txt, focusLost)
    selectedtime3 = (txt)
end)

section2:addKeybind("Autoshoot", Enum.KeyCode.Clear, function()
    time = (selectedtime3)
    
    local args = {
        [1] = "Shoot",
        [2] = true
    }
    
    game:GetService("ReplicatedStorage").GameEvents.ClientAction:FireServer(unpack(args))
    wait(time)
    
    local args = {
        [1] = "Shoot",
        [2] = false
    }
    
    game:GetService("ReplicatedStorage").GameEvents.ClientAction:FireServer(unpack(args))
end)

section10:addTextbox("Hand hitbox", "", function(s, focusLost)
    game:GetService("Workspace")[game.Players.LocalPlayer.Name].RightHand.Size = Vector3.new(s, s, s)
    game:GetService("Workspace")[game.Players.LocalPlayer.Name].LeftHand.Size = Vector3.new(s, s, s)
end)

section10:addTextbox("Change name", "", function(value, focusLost)
    for i,v in ipairs(game:GetService("Workspace").NameUIFolder:GetDescendants()) do
        if v:IsA("TextLabel") then
            v.Text = (value)
        end
    end
end)

section10:addTextbox("Fov unlocker", "", function(value, focusLost)
    workspace.CurrentCamera.FieldOfView = (value)
end)

local page = venyx:addPage("Dribbles", 5012544693)
local section3 = page:addSection("Keybinds")

section3:addKeybind("Right Hesi Spin", Enum.KeyCode.Clear, function()
    local ohString1 = "Dribble Move"
    local ohString2 = "CV"
